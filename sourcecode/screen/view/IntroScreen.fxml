<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="780.0" fitWidth="1024.0">
         <image>
            <Image url="@../images/intro-screen.png" />
         </image>
      </ImageView>
      <Button fx:id="btnStartGame" layoutX="419.0" layoutY="212.0" mnemonicParsing="false" onAction="#btnStartGameClicked" opacity="0.0" prefHeight="101.0" prefWidth="187.0" text="Start">
         <font>
            <Font size="36.0" />
         </font>
      </Button>
      <Button fx:id="btnAccessHelp" layoutX="408.0" layoutY="376.0" mnemonicParsing="false" onAction="#btnAccessHelpClicked" opacity="0.0" prefHeight="101.0" prefWidth="210.0" text="Help">
         <font>
            <Font size="36.0" />
         </font>
      </Button>
      <Button fx:id="btnExitGame" layoutX="419.0" layoutY="536.0" mnemonicParsing="false" onAction="#btnExitGameClicked" opacity="0.0" prefHeight="101.0" prefWidth="203.0" text="Exit">
         <font>
            <Font size="36.0" />
         </font>
      </Button>
      <Rectangle fx:id="Mute" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="51.0" layoutX="829.0" layoutY="45.0" stroke="BLACK" strokeType="INSIDE" width="119.0" />
      <Circle fx:id="unMute" fill="DODGERBLUE" layoutX="892.0" layoutY="66.0" radius="48.0" stroke="BLACK" strokeType="INSIDE" />
      <Button fx:id="adjustMusicButton" layoutX="852.0" layoutY="47.0" mnemonicParsing="false" onMouseClicked="#adjustMusic" prefHeight="38.0" prefWidth="86.0" text="Button" />
   </children>
</AnchorPane>
